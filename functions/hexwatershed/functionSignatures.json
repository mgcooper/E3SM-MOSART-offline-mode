      "inputs":
      [
         {"name":"jsonfile",  "kind":"required",   "type":["char"]},
         {"name":"savefile",  "kind":"namevalue",  "type":["logical","scalar"],"purpose":"Option"},
         {"name":"filename",  "kind":"namevalue",  "type":["char"]}
      ]
   },
   "pyflowlinejson2shp":
   {
      "inputs":
      [
         {"name":"geojsonfile",  "kind":"required",   "type":["char"]},
